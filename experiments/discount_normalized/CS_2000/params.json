{
    "dataset_dir": "data/pathways_datasets/CS",
    "graph_size": 2000,

    "filters": {
        "students": {
            "min_courses": 4
        },
        "courses": {
            "min_enrollment": 6,
            "ignore": [
                "IHUM*",
                "PWR*",
                "THINK*",
                "OSP*",
                "ATHLETIC*",
                "WCT*"
            ]
        }
    },

    "model_class": "ExponentialDiscountNormalized",
    "model_params": {
        "gamma": 0.3
    }
}